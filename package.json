{
  "name": "disenador",
  "version": "1.0.0",
  "description": "Dise√±ador de logos",
  "main": "app.js",
  "scripts": {
    "build-css": "node-sass --include-path public public/creador-de-logos/assets/scss -o public/creador-de-logos/assets/css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "build-css-landing": "node-sass --include-path public public/landing/assets/scss -o public/landing/assets/css",
    "watch-css-landing": "nodemon -e scss -x \"npm run build-css-landing\"",
    "start": "npm-run-all --parallel watch:server watch:build",
    "start-dev": "npm-run-all --parallel watch:server watch:build-dev",
    "watch:build": "webpack --watch --mode production --config webpack.config.js",
    "watch:build-dev": "webpack --watch --mode development --config webpack.config.js",
    "watch:server": "nodemon app.js --watch \"./dist\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BazamIdeas/disenador.git"
  },
